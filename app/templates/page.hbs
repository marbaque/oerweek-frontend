{{ui-hooks
        did-insert=(action 'activateFaq' model.slug)
        will-destroy=(action 'deactivateFaq' model.slug)}}

<div class="bg-gray-900">
  <div class="container">
    <div class="hidden lg:block py-5 pt-20">Home | Pages </div>
    <div class="lg:w-7/12 lg:ml-32 mt-10 pb-20 content">
      <h1>{{{model.title}}}</h1>
      <div class="prose">
        {{{model.content}}}
      </div>
    </div>
  </div>
</div>
